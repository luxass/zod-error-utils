{
  "name": "zod-error-utils",
  "version": "0.3.0",
  "description": "Zod error utilities",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.11.0",
  "license": "MIT",
  "homepage": "https://github.com/luxass/zod-error-utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/zod-error-utils.git"
  },
  "keywords": [
    "zod"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./error-map": {
      "import": {
        "types": "./dist/error-map.d.ts",
        "default": "./dist/error-map.mjs"
      },
      "require": {
        "types": "./dist/error-map.d.cts",
        "default": "./dist/error-map.cjs"
      }
    },
    "./flatten": {
      "import": {
        "types": "./dist/flatten.d.ts",
        "default": "./dist/flatten.mjs"
      },
      "require": {
        "types": "./dist/flatten.d.cts",
        "default": "./dist/flatten.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "peerDependencies": {
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.18.1",
    "@types/node": "^22.0.0",
    "eslint": "^9.27.0",
    "eslint-plugin-format": "^1.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4",
    "zod": "^3.25.34"
  }
}
