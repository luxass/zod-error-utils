{
  "name": "zod-error-utils",
  "version": "0.3.0",
  "description": "Zod error utilities",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.11.0",
  "license": "MIT",
  "homepage": "https://github.com/luxass/zod-error-utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/zod-error-utils.git"
  },
  "keywords": [
    "zod"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./error-map": {
      "import": "./dist/error-map.js",
      "require": "./dist/error-map.cjs"
    },
    "./flatten": {
      "import": "./dist/flatten.js",
      "require": "./dist/flatten.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "peerDependencies": {
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.18.1",
    "@types/node": "^22.0.0",
    "eslint": "^9.27.0",
    "eslint-plugin-format": "^1.0.1",
    "publint": "^0.3.12",
    "tsdown": "^0.12.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4",
    "zod": "^3.25.34"
  }
}
